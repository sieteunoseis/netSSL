{
  "name": "netssl-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec \"node --env-file ../.env -r ts-node/register src/server.ts\"",
    "nodemon": "nodemon --exec ts-node src/server.ts",
    "type-check": "tsc --noEmit",
    "test": "jest --testPathIgnorePatterns=integration",
    "test:watch": "jest --watch --testPathIgnorePatterns=integration",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPatterns=integration",
    "clear-stuck-operations": "node --env-file ../.env -r ts-node/register src/clear-stuck-operations.ts"
  },
  "dependencies": {
    "@aws-sdk/client-route-53": "^3.400.0",
    "@azure/arm-dns": "^5.1.0",
    "@azure/identity": "^4.0.0",
    "@google-cloud/dns": "^4.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io": "^3.0.1",
    "@types/ssh2": "^1.15.5",
    "acme-client": "^5.4.0",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "node-cron": "^4.2.1",
    "node-forge": "^1.3.1",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.6",
    "ssh2": "^1.16.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.4",
    "@types/node-forge": "^1.3.13",
    "@types/sqlite3": "^3.1.8",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.2",
    "jest": "^30.0.3",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
